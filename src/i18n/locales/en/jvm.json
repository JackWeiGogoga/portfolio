  "runtimeAreaDetails": {
    "heap": {
      "summary": "Shared memory area responsible for object and array allocation and reclamation.",
      "tabs": [
        {
          "id": "overview",
          "label": "Overview",
          "items": [
            "Generational management: young/old; most GC happens in young gen.",
            "String pool: moved to heap in JDK 7+, separated from method area.",
            "Allocation: TLAB for thread-local fast allocation, fallback to shared allocation."
          ]
        },
        {
          "id": "structure",
          "label": "Structure",
          "diagram": [
            "Young Gen",
            "  ├─ Eden",
            "  ├─ S0 (From)",
            "  └─ S1 (To)",
            "Old Gen",
            "  └─ Tenured"
          ],
          "items": ["Objects go to Eden first; survivors copy between Survivor spaces."]
        },
        {
          "id": "versions",
          "label": "Version differences",
          "items": [
            "JDK 6/7: string pool in PermGen/method area.",
            "JDK 7+: string pool moved to heap to reduce PermGen pressure."
          ]
        },
        {
          "id": "config",
          "label": "Config",
          "items": [
            "-Xms/-Xmx: initial/max heap size",
            "-Xmn: young generation size",
            "-XX:NewRatio: young/old ratio",
            "-XX:SurvivorRatio: Eden/Survivor ratio",
            "-XX:MaxTenuringThreshold: promotion age"
          ]
        },
        {
          "id": "exceptions",
          "label": "Exceptions",
          "items": [
            "OutOfMemoryError: Java heap space",
            "GC overhead limit exceeded"
          ]
        }
      ]
    },
    "method-area": {
      "summary": "Method area is a JVM spec concept that stores class metadata and runtime constants.",
      "tabs": [
        {
          "id": "overview",
          "label": "Overview",
          "items": [
            "Stores class metadata, runtime constant pool, static fields, JIT code.",
            "Unlike heap: holds class-level data, shared by threads."
          ]
        },
        {
          "id": "versions",
          "label": "Implementations",
          "items": [
            "PermGen (<= JDK 7): inside heap, prone to OOM, fixed size.",
            "Metaspace (JDK 8+): native memory, grows on demand.",
            "Why replaced: class metadata size unpredictable; PermGen tuning is costly."
          ]
        },
